{% extends "layout.html.twig" %}

{% block title %}
    Page d'acceuil |
    {{ parent() }}
{% endblock %}

{% block body %}
    <h1>Page d'acceuil de mon app Symfony</h1>
    
    {{ dump(data) }}

    <p>
        Nom :
        {{ data.nom }}    
    </p>
    <p>
        Age :
        {{ data.age }}    
    </p>
    <p>
        Ville :
        {{ data.ville }}    
    </p>

{% endblock %}

